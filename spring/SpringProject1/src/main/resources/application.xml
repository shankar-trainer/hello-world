<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="org.tvscredit.model.Product">
		<property name="prdId" value="67876"></property>
		<property name="prdLocation" value="chennai"></property>
		<property name="bikeList">
			<list>
				<value>Yamaha</value>
				<value>Hero Splendour</value>
				<value>Honda Shine </value>
				<value>Pulsar </value>
				<value>Bullet </value>
				<value>Hero Splendour</value>
			</list>

		</property>
		<property name="grocerySet">
			<set>
				<value>wheat flour</value>
				<value>sugar</value>
				<value>tea power</value>
				<value>salt</value>
				<value>sugar</value>
			</set>
		</property>
		<property name="customerMap">
			<map>
				<entry key="1" value-ref="customer1"></entry>
				<entry key="2" value-ref="customer2"></entry>
				<entry key="3" value-ref="customer3"></entry>
			</map>
		</property>
	</bean>
	<bean id="customer1" class="org.tvscredit.model.Customer">
		<property name="custId" value="10011"></property>
		<property name="custName" value="virendra kumar"></property>
		<property name="custSalary" value="90000"></property>
	</bean>

	<bean id="customer2" class="org.tvscredit.model.Customer">
		<property name="custId" value="10012"></property>
		<property name="custName" value="amrendra kumar"></property>
		<property name="custSalary" value="98000"></property>
	</bean>

	<bean id="customer3" class="org.tvscredit.model.Customer">
		<property name="custId" value="10013"></property>
		<property name="custName" value="dharmendra kumar"></property>
		<property name="custSalary" value="40000"></property>
	</bean>

</beans>
