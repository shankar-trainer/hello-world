<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	
	
	<!--  auto wiring  -->
	
<!-- 	<bean id="bus3" class="edu.example.model.Bus" autowire="byType"> -->
	<bean id="bus3" class="edu.example.model.Bus" autowire="byName">
		<property name="regNo" value="98990"></property>
		<property name="busType" value="tamil Nadu state Bus"></property>
		<property name="fare" value="800"></property>
	</bean>
	
<!-- 	<bean id="driver3" class="edu.example.model.Driver"> -->
	<bean id="driver" class="edu.example.model.Driver">
		<property name="id" value="671"></property>
		<property name="name" value="naveen selva"></property>
		<property name="age" value="46"></property>
		<property name="salary" value="35000"></property>
	</bean>
	
	

</beans>