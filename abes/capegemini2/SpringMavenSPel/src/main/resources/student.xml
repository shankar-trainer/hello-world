<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">
	<context:annotation-config />
	<context:component-scan
		base-package="org.com.spel" />

	<bean id="st1" class="org.com.spel.Student">
		<property name="roll" value="6776"></property>
		<property name="name" value="ashish bansal"></property>
		<property name="marks" value="78.56"></property>
	</bean>

	<bean id="course1" class="org.com.spel.Course">
		<property name="course_id" value="1267"></property>
		<property name="course_name" value="B.Tech"></property>
		<property name="course_duration" value="4"></property>
	</bean>
	
	
 	<bean id="course2" class="org.com.spel.Course">
		<property name="course_id" value="1269"></property>
		<property name="course_name" value="M.Tech"></property>
		<property name="course_duration" value="2"></property>
	</bean>
	





</beans>