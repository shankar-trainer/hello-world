<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">


	<bean id="book1" class="org.com.model.Book" autowire="constructor"
		init-method="init" destroy-method="clean" lazy-init="true">
		<constructor-arg name="book_isbn" value="766776"></constructor-arg>
		<constructor-arg name="book_name" value="kids story"></constructor-arg>
		<constructor-arg name="book_cost" value="300"></constructor-arg>
	</bean>

	<bean id="wb" class="org.com.model.WaterBottle" scope="prototype" >
		<property name="wb_id" value="1"></property>
		<property name="wb_color" value="blue"></property>
		<property name="wb_type" value="plastic"></property>
	</bean>


</beans>