<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN" "http://struts.apache.org/dtds/xwork-validator-1.0.2.dtd">
        
        
<validators>
	<field name="prdId">
		<field-validator type="int">
			<param name="min">1000</param>
			<param name="max">10000</param>
			<message>Min 1000 and Max 10000 is allowe</message>
		</field-validator>
	</field>

	<field name="prdCost">
		<field-validator type="float">
			<param name="min">500</param>
			<param name="max">2000</param>
			<message>min 500 and max 2000 is allowed</message>
		</field-validator>
	</field>

	<field name="prdName">
		<field-validator type="requiredstring">
			<message key="error.prdname.blank"></message>
		</field-validator>
	</field>

</validators>