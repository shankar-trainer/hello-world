<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">
	<bean id="customer1" class="org.com.model.Customer"
		autowire="byType">
		<property name="cid" value="776677"></property>
		<property name="name" value="mahendra kumar" />
		<property name="ssn" value="ssn1234"></property>
		<!-- <property name="phone" ref="phone1"></property> -->
	</bean>

	<bean id="phone1" class="org.com.model.Phone">
		<property name="phone_id" value="56778" />
		<property name="phone_model" value="samsung" />
		<property name="phone_cost" value="50000" />
	</bean>
 
</beans>