<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
		"-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
		"http://struts.apache.org/dtds/struts-2.5.dtd">
<struts>
	<constant name="struts.devMode" value="false" />

	<package name="basicstruts2" extends="struts-default">

		<action name="myaction" class="com.example.model.User">
			<result name="success">result/success.jsp</result>
			<result name="error">result/error.jsp</result>
		</action>

		<action name="empAction" class="com.example.model.Employee">
			<result name='success'>WEB-INF/empResult/empdata.jsp</result>
			<result name='error'>EmpForm.jsp</result>
		</action>

		<action name="surveyAction" class="com.example.model.Survey">
			<result name='success'>WEB-INF/surveyResult/surveydata.jsp</result>
			<result name='input'>SurveyForm.jsp</result>
		</action>
		
		<action name="actAction" class="com.example.model.Account">
			<result name='success'>accountResult.jsp</result>
			<result name='input'>accountForm.jsp</result>
		</action>

	</package>
</struts>
