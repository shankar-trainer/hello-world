<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="user1" class="com.cts.model.User">
		<property name="userId" value="566566" />
		<property name="userName" value="r kumar" />
		<property name="userLocation" value="delhi" />
		<property name="prdList">

			<list>
				<value>java book</value>
				<value>mobile</value>
				<value>laptop</value>
				<value>mobile</value>
				<value>car</value>
			</list>
		</property>
		<property name="cityList">
			<set>
				<value>delhi</value>
				<value>kolkotta</value>
				<value>mumbai</value>
				<value>delhi</value>
			</set>
		</property>
		<property name="bookList">
			<set>
				<ref bean="book1" />
				<ref bean="book2" />
				<ref bean="book3" />
			</set>
		</property>
	</bean>

	<bean class="com.cts.model.Book" id="book1">
		<property name="isbn" value="455656"></property>
		<property name="author" value="vivek kumar"></property>
		<property name="bname" value="java in action"></property>
		<property name="cost" value="1200"></property>
	</bean>

	<bean class="com.cts.model.Book" id="book2">
		<property name="isbn" value="455659"></property>
		<property name="author" value="shashi kumar"></property>
		<property name="bname" value="angular in action"></property>
		<property name="cost" value="3200"></property>
	</bean>

	<bean class="com.cts.model.Book" id="book3">
		<property name="isbn" value="455657"></property>
		<property name="author" value="amit kumar"></property>
		<property name="bname" value="spring in action"></property>
		<property name="cost" value="2200"></property>
	</bean>
</beans>