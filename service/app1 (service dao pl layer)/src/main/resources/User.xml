<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="user1" class="org.com.collection.User">
		<property name="id" value="70000"></property>
		<property name="name" value="amrendra"></property>
		<property name="bike">
			<set>
				<value>scooty</value>
				<value>bullet</value>
			</set>
		</property>
		<property name="mobile">
			<map>
			<entry key="1" value="samsung"></entry>
			<entry key="2" value="nokia"></entry>
			</map>
		</property>
		
		<property name="car">
			<list>
				<value>honda city</value>
				<value>i10</value>
				<value>jaguar</value>
			</list>
		</property>



	</bean>








</beans>