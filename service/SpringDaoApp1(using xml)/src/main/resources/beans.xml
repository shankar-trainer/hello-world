<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="dao" class="org.com.dao.EmployeeDaoImpl">
    </bean>

    <bean id="service" class="org.com.service.EmployeeService">
        <property name="dao" ref="dao"></property>
    </bean>

    <bean id="emp1" class="org.com.model.Employee">
        <property name="id" value="1001"></property>
        <property name="name" value="ram kumar"></property>
    </bean>
    <bean id="emp2" class="org.com.model.Employee">
        <property name="id" value="1002"></property>
        <property name="name" value="suresh kumar"></property>
    </bean>
    <bean id="emp3" class="org.com.model.Employee">
        <property name="id" value="1001"></property>
        <property name="name" value="vivek kumar"></property>
    </bean>



</beans>