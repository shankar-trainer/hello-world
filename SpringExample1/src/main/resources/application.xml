<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xmlns:c="http://www.springframework.org/schema/c"

	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="book1" class="org.capg.model.Book" init-method="init"
		destroy-method="cleanup" lazy-init="true">
		<property name="isbn" value="677676767"></property>
		<property name="bname" value="spring in action"></property>
		<property name="cost" value="2000"></property>
	</bean>

	<bean id="book2" class="org.capg.model.Book" p:isbn="87878"
		p:bname="java beginner" p:cost="789">
	</bean>

	<bean id="person2" class="org.capg.model.Person"
		autowire="constructor" c:personId="6565665" c:personName="amit kumar">
	</bean>

	<bean id="person1" class="org.capg.model.Person"
		autowire="constructor">
		<constructor-arg name="personId" value="676776" />
		<constructor-arg name="personName" value="smitha d" />
		<!-- <constructor-arg name="bday" ref="birthday" /> -->
	</bean>

	<bean id="birthday" class="org.capg.model.PersonBirthday">
		<constructor-arg name="day" value="23" />
		<constructor-arg name="month" value="12" />
		<constructor-arg name="year" value="1999" />
	</bean>

	<bean id="passenger1" class="org.capg.model.Passenger">
		<property name="passengerId" value="7776"></property>
		<property name="passengerName" value="sangeetha t"></property>
		<property name="passengerEmail" value="sangeetha_t@gmail.com"></property>
		<property name="cityList">
			<list>
				<value>chennai</value>
				<value>madurai</value>
				<value>rameswaram</value>
				<value>kashi</value>
				<value>paryag</value>
			</list>
		</property>
		<property name="stateSet">
			<set>
				<value>Bihar</value>
				<value>Uttar Pardesh</value>
				<value>Tamil Nadu</value>
				<value>Jharkhand</value>
				<value>Karnataka</value>
				<value>Andhra Pardesh</value>
			</set>
		</property>

		<property name="countrycapital">
			<map>
				<entry key="india" value="new delhi" />
				<entry key="srilanka" value="cylone" />
				<entry key="nepal" value="kathmandu" />
				<entry key="china" value="sanghai" />
				<entry key="japan" value="tokyo" />
			</map>
		</property>
		<property name="cityTicket">
			<props>
				<prop key="delhi">1500</prop>
				<prop key="banaras">1200</prop>
				<prop key="chennai">1700</prop>
				<prop key="banglore">1800</prop>
				<prop key="mumbai">2000</prop>
			</props>
		</property>

	</bean>

</beans>
