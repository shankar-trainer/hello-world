<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="book1" class="app1.Book">
		<property name="isbn" value="565665" ></property>
		<property name="book_name" value= "spring in action "></property>
	</bean>

	<bean id="chapter1" class="app1.Chapter">
		<property name="pageno" value="1" ></property>
		<property name="chapter_name" value= "spring basics "></property>
	</bean>
	
	
	<bean id="chapter2" class="app1.Chapter">
		<property name="pageno" value="50" ></property>
		<property name="chapter_name" value= "spring di "></property>
	</bean>






</beans>